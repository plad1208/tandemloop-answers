import java.util.Scanner;

public class problem3 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int n = (a % 2 == 1) ? a : a - 1;
        int i = 1;
        while (i <= n) {
            System.out.print(2 * i - 1);
            if (i < n) {
                System.out.print(" ");
            }
            i++;
        }
    }
}
